  "license": "GPLv3",
  "browserslist": [
    "> 0.5% in BR, Firefox ESR, not dead"
  ],
  "dependencies": {
    "@babel/runtime": "7.13.x",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@reduxjs/toolkit": "1.5.0",
    "axios": "0.21.0",
    "babel-loader": "8.2.3",
    "chart.js": "2.9.4",
    "core-js": "3.19.1",
    "css-loader": "6.5.1",
    "file-loader": "6.2.0",
    "file-saver": "1.3.8",
    "node-sass": "7.0.0",
    "react": "16.9.0",
    "react-datepicker": "2.9.6",
    "react-dom": "16.9.0",
    "react-is": "17.0.1",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "regenerator-runtime": "0.13.7",
    "sass-loader": "12.3.0",
    "style-loader": "3.3.1",
    "styled-components": "5.2.1",
    "styled-icons": "10.22.0",
    "ts-loader": "9.2.6",
    "xlsx": "0.16.2"
  },
  "devDependencies": {
    "@babel/cli": "7.13.x",
    "@babel/core": "7.13.x",
    "@babel/node": "7.13.x",
    "@babel/plugin-proposal-class-properties": "7.13.x",
    "@babel/plugin-transform-async-to-generator": "7.13.x",
    "@babel/plugin-transform-modules-commonjs": "7.13.x",
    "@babel/plugin-transform-runtime": "7.13.x",
    "@babel/preset-env": "7.13.x",
    "@babel/preset-react": "7.13.x",
    "@babel/preset-typescript": "7.13.x",
    "@babel/register": "7.13.x",
    "@types/chart.js": "2.9.4",
    "@types/file-saver": "2.0.1",
    "@types/react": "17.0.1",
    "@types/react-datepicker": "3.1.3",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.16",
    "@types/redux-logger": "3.0.8",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "babel-eslint": "10.1.0",
    "compression-webpack-plugin": "6.1.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "2.6.0",
    "html-webpack-plugin": "4.5.2",
    "madge": "4.0.2",
    "path": "0.12.7",
    "prettier": "2.2.1",
    "simplify-js": "1.2.3",
    "typescript": "4.1.3",
    "webpack": "5.64.x",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "3.11.0"
  },
  "madge": {
    "fileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ]
  },
  "scripts": {
    "update-browserslist": "npx browserslist@latest --update-db",
    "info": "npm run info:circular; npm run info:orphans; npm run info:leaves",
    "info:deps": "npx madge --image graph.svg ./src/",
    "info:circular": "npx madge --circular ./src/  || true",
    "info:orphans": "npx madge --orphans ./src/  || true",
    "info:leaves": "npx madge --leaves ./src/ || true",
    "update-version": "sed -i -e \"s,const version =.*,const version = \\\"$(date -I)-$(git rev-parse --short HEAD)\\\";,g\" ./src/components/Footer/index.tsx",
    "lint": "npx eslint src/lib/*",
    "build": "rm -rfv build/*; npm run update-version; NODE_ENV=production npx webpack",
    "start": "npm run update-version; NODE_ENV=development npx webpack serve --host 127.0.0.1 --https",
    "build:analyze": "npm run build -- --env.addons=bundleanalyzer"
  }
}
